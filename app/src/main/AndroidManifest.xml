<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="barcan.virgil.com.shopassistant" >

    <uses-sdk android:minSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme" >
        <activity
            android:name=".frontend.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".frontend.regular.UserMainScreenActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".frontend.regular.UserRegisterActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".frontend.company.CompanyRegisterActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".frontend.regular.UserShoppingListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".frontend.ShowProductActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
